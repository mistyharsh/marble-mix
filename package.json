{
    "name": "marble-mix",
    "version": "0.6.0",
    "description": "Utilities for Marble.js",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "tsc && cp package*.json dist/ && cp README.md dist/",
        "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
        "peer": "npm install --no-save @marblejs/core rxjs",
        "test": "jest",
        "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mistyharsh/marble-mix.git"
    },
    "author": "Harshal Patil",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mistyharsh/marble-mix/issues"
    },
    "homepage": "https://github.com/mistyharsh/marble-mix#readme",
    "keywords": [
        "marblejs",
        "rxjs",
        "static",
        "marblejs-plugin"
    ],
    "dependencies": {
        "@types/send": "^0.14.4",
        "chalk": "^2.4.1",
        "graphql-playground-html": "^1.6.13",
        "parseurl": "^1.3.2",
        "peer": "^0.2.10",
        "send": "^0.16.2",
        "statehood": "^6.0.8",
        "table": "^5.1.0"
    },
    "peerDependencies": {
        "@marblejs/core": "*",
        "rxjs": "*"
    },
    "devDependencies": {
        "@types/jest": "^23.3.9",
        "@types/node": "^10.12.9",
        "@types/parseurl": "^1.3.1",
        "@types/supertest": "^2.0.6",
        "@types/table": "^4.0.5",
        "coveralls": "^3.0.2",
        "jest": "^23.6.0",
        "rxjs-tslint": "^0.1.5",
        "supertest": "^3.3.0",
        "ts-jest": "^23.10.4",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.1.6"
    }
}
